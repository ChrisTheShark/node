{
  "name": "ChrisTheShark-nodejs",
  "version": "0.1.3",
  "author": "ChrisTheShark",
  "summary": "Puppet module to install nodejs globally on Centos or Ubuntu Linux flavors.",
  "license": "Apache-2.0",
  "source": "https://github.com/ChrisTheShark/nodejs",
  "project_page": "https://github.com/ChrisTheShark/nodejs",
  "issues_url": "https://github.com/ChrisTheShark/nodejs/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">=1.0.0 <5.0.0"
    }
  ]
}
